{
    "name": "anfragen/laravel-2fa-breeze",
    "description": "Building 2FA functionality for the Laravel.",
    "keywords": [
        "anfragen",
        "laravel",
        "breeze",
        "2fa",
        "auth"
    ],
    "homepage": "https://github.com/Anfragen/laravel-2fa-breeze",
    "license": "MIT",
    "authors": [
        {
            "name": "Jo√£o Pedro Lopes",
            "email": "joaopalopes24@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^9.0",
        "pragmarx/google2fa": "^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "laravel/vonage-notification-channel": "^3.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.22",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.16",
        "phpunit/phpunit": "^9.0",
        "pestphp/pest-plugin-laravel": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Anfragen\\TwoFactor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anfragen\\TwoFactor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anfragen\\TwoFactor\\TwoFactorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
